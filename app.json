{
  "expo": {
    "scheme": "studystacks",
    "name": "StudyStacks",
    "slug": "StudyStacks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/darkBrain.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/img/darkBrain.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/darkBrain.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/img/darkBrain.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e9bd6611-8bc1-47e1-b5c0-7fae837a54b5"
      }
    },
    "owner": "colthorp789"
  }
}
